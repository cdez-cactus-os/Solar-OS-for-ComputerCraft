term.clear()
term.setCursorPos(1, 1)
if fs.exsist("/install") then
  fs.delete("/install")
end
if fs.exists("/eject") then
  fs.delete("/eject")
end
fs.copy("/disk/install","/install")
fs.copy("/disk/eject","/eject")
print("First time installation? Type 'install' to begin. If not, type eject.")
